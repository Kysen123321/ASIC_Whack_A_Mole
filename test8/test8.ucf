NET "clk" LOC = T8 | TNM_NET = sys_clk_pin;
TIMESPEC TS_sys_clk_pin = PERIOD sys_clk_pin 50000 kHz;
NET "rst_n" CLOCK_DEDICATED_ROUTE = FALSE;
##
NET rst_n                LOC = L3 | IOSTANDARD = "LVCMOS33";         ## reset pushbutton
########LED Pin define#####################
NET dispL<0>                LOC = T12 | IOSTANDARD = "LVCMOS33";       ## LED1
NET dispL<1>                LOC = R9 | IOSTANDARD = "LVCMOS33";       ## LED2
NET dispL<2>                LOC = T9 | IOSTANDARD = "LVCMOS33";       ## LED3
NET dispL<3>                LOC = L8 | IOSTANDARD = "LVCMOS33";       ## LED4
NET dispL<4>                LOC = T7 | IOSTANDARD = "LVCMOS33";       ## LED5
NET dispL<5>                LOC = R7 | IOSTANDARD = "LVCMOS33";       ## LED6
NET dispL<6>                LOC = T6 | IOSTANDARD = "LVCMOS33";       ## LED7
NET dispL<7>                LOC = T5 | IOSTANDARD = "LVCMOS33";       ## LED8
########KEY Pin define#####################
NET key_out_x<0>             LOC = J16 | IOSTANDARD = "LVCMOS33";      

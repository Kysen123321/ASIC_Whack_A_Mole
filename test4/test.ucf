NET "clk" LOC = T8 | TNM_NET = sys_clk_pin;
TIMESPEC TS_sys_clk_pin = PERIOD sys_clk_pin 50000 kHz;
NET "reset" CLOCK_DEDICATED_ROUTE = FALSE;
##
NET reset                LOC = L3 | IOSTANDARD = "LVCMOS33";         ## reset pushbutton
########LED Pin define#####################
NET dispL<0>             LOC = P4 | IOSTANDARD = "LVCMOS33";       ## LED1
NET dispL<1>             LOC = N5 | IOSTANDARD = "LVCMOS33";       ## LED2
NET dispL<2>             LOC = P5 | IOSTANDARD = "LVCMOS33";       ## LED3
NET dispL<3>             LOC = M6 | IOSTANDARD = "LVCMOS33";       ## LED4
########KEY Pin define#####################
NET button<0>            LOC = C3 | IOSTANDARD = "LVCMOS33";       ## KEY1
NET button<1>            LOC = D3 | IOSTANDARD = "LVCMOS33";       ## KEY2
NET button<2>            LOC = E4 | IOSTANDARD = "LVCMOS33";       ## KEY3
NET button<3>            LOC = E3 | IOSTANDARD = "LVCMOS33";       ## KEY4
########seg pin define#####################
NET segsig<0>            LOC = C7 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET segsig<1>            LOC = E6 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST; 
NET segsig<2>            LOC = C5 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET segsig<3>            LOC = F7 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET segsig<4>            LOC = D6 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET segsig<5>            LOC = E7 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET segsig<6>            LOC = D5 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET segsig<7>            LOC = C6 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST; 
NET bitsig<0>            LOC = D9 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET bitsig<1>            LOC = E10 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET bitsig<2>            LOC = F10 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET bitsig<3>             LOC = F9 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET bitsig<4>             LOC = E8 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST;
NET bitsig<5>             LOC = D8 | IOSTANDARD = "LVCMOS33"  | SLEW=FAST; 

NET "clk" LOC = T8 | TNM_NET = sys_clk_pin;
TIMESPEC TS_sys_clk_pin = PERIOD sys_clk_pin 50000 kHz;

##
NET reset                LOC = L3 | IOSTANDARD = "LVCMOS33";         ## reset pushbutton
########LED Pin define#####################
NET dispL<0>             LOC = P4 | IOSTANDARD = "LVCMOS33";       ## LED1
